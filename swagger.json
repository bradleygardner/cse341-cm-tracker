{
	"openapi": "3.0.0",
	"info": {
		"title": "Personal API",
		"version": "1.0.0",
		"description": "API documentation for the Personal API"
	},
	"servers": [
		{
			"url": "http://localhost:5500",
			"description": "Local server"
		}
	],
	"paths": {
		"/auth": {
			"get": {
				"summary": "Get authentication status",
				"responses": {
					"200": {
						"description": "Authentication status"
					}
				}
			}
		},
		"/auth/google": {
			"get": {
				"summary": "Authenticate with Google",
				"responses": {
					"302": {
						"description": "Redirect to Google for authentication"
					}
				}
			}
		},
		"/auth/google/callback": {
			"get": {
				"summary": "Google authentication callback",
				"responses": {
					"302": {
						"description": "Redirect after Google authentication"
					}
				}
			}
		},
		"/auth/logout": {
			"get": {
				"summary": "Logout user",
				"responses": {
					"200": {
						"description": "Logout successful"
					}
				}
			}
		},
		"/car": {
			"get": {
				"summary": "Get all cars",
				"responses": {
					"200": {
						"description": "List of cars"
					}
				}
			},
			"post": {
				"summary": "Create a new car",
				"responses": {
					"201": {
						"description": "Car created"
					}
				}
			}
		},
		"/car/{id}": {
			"get": {
				"summary": "Get a car by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Car details"
					}
				}
			},
			"put": {
				"summary": "Update a car by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Car updated"
					}
				}
			},
			"delete": {
				"summary": "Delete a car by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Car deleted"
					}
				}
			}
		},
		"/maintenance": {
			"get": {
				"summary": "Get all maintenance records",
				"responses": {
					"200": {
						"description": "List of maintenance records"
					}
				}
			},
			"post": {
				"summary": "Create a new maintenance record",
				"responses": {
					"201": {
						"description": "Maintenance record created"
					}
				}
			}
		},
		"/maintenance/{id}": {
			"get": {
				"summary": "Get a maintenance record by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Maintenance record details"
					}
				}
			},
			"put": {
				"summary": "Update a maintenance record by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Maintenance record updated"
					}
				}
			},
			"delete": {
				"summary": "Delete a maintenance record by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Maintenance record deleted"
					}
				}
			}
		},
		"/warranty": {
			"get": {
				"summary": "Get all warranties",
				"responses": {
					"200": {
						"description": "List of warranties"
					}
				}
			},
			"post": {
				"summary": "Create a new warranty",
				"responses": {
					"201": {
						"description": "Warranty created"
					}
				}
			}
		},
		"/warranty/{id}": {
			"get": {
				"summary": "Get a warranty by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Warranty details"
					}
				}
			},
			"put": {
				"summary": "Update a warranty by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Warranty updated"
					}
				}
			},
			"delete": {
				"summary": "Delete a warranty by ID",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Warranty deleted"
					}
				}
			}
		}
	}
}
